<Window x:Class="LibraryProject.Views.Worker.WorkerTabs.ItemPropsView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LibraryProject.Views.Worker.WorkerTabs"
        mc:Ignorable="d"
        Title="ItemPropsView" Height="150" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <TextBlock x:Name="first" Grid.Row="0"/>
        <TextBox x:Name="link" IsReadOnly="True" Grid.Row="1"/>
        <TextBlock x:Name="last" Grid.Row="2"/>
    </Grid>
</Window>
